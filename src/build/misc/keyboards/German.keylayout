			<!-- down -->
			<key code="125" output="&#x1F;"/>
			<!-- up -->
			<key code="126" output="&#x1E;"/>
		</keyMap>
		<keyMap index="2">
			<!-- option (ALT) -->
			<!-- Alt + l : @-sign -->
			<key code="37" output="&#x0040;"/>
		</keyMap>
	</keyMapSet>
</keyboard>
