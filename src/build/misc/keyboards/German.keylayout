			<key code="99" output="&#x10;"/>
			<key code="100" output="&#x10;"/>
			<key code="101" output="&#x10;"/>
			<key code="102" output=" "/>
			<key code="103" output="&#x10;"/>
			<key code="104" output=" "/>
			<key code="105" output="&#x10;"/>
